@model List<P134Lab.Models.Student>
@{
    ViewData["Title"] = "Index";
}

<div class="container mt-4">
    <table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name1</th>
            <th>Surname</th>
            <th>Age</th>
            <th>Settings</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Surname</td>
                <td>@item.Age</td>
                <td>
                    <a class="btn btn-outline-danger" href="Home/Remove/@item.Id">Remove</a>
                    <a class="btn btn-outline-primary" href="Home/Update/@item.Id">Update</a>
                </td>

            </tr>
        }
    </tbody>
</table>
<a class="btn btn-outline-info" href="Home/Create">Create</a>
</div>
